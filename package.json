{
    "name": "gitbook-plugin-meta-single",
    "description": "Add single metadata to <head> for your gitbook",
    "version": "0.1.13",
    "engines": {
        "gitbook": "*"
    },
    "homepage": "https://github.com/CyberZHG/gitbook-plugin-meta",
    "repository": {
        "type": "git",
        "url": "https://github.com/CyberZHG/gitbook-plugin-meta.git"
    },
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/CyberZHG/gitbook-plugin-meta/issues"
    },
    "gitbook": {
        "properties": {
            "name": {
                "type": "string",
                "required": true,
                "default": ""
            },
            "content": {
                "type": "string",
                "required": true,
                "default": ""
            }
        }
    }
}
