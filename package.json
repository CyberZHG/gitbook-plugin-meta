{
    "name": "gitbook-plugin-meta",
    "description": "Add meta data to <head> for your gitbook",
    "version": "0.0.5",
    "engines": {
        "gitbook": "*"
    },
    "homepage": "https://github.com/CyberZHG/gitbook-plugin-meta",
    "repository": {
        "type": "git",
        "url": "https://github.com/CyberZHG/gitbook-plugin-meta.git"
    },
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/CyberZHG/gitbook-plugin-meta/issues"
    },
    "gitbook": {
        "properties": {
            "data": {
                "type": "array",
                "title": "Metadata",
                "description": "An array of meta data.",
                "default": []
            }
        }
    }
}
